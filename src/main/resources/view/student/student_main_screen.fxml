<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.se.quiz.quiz_management_system.controller.student.StudentMainScreenController">
   <left>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#btnLogoutPressed" text="Logout">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="btnQuizzes" mnemonicParsing="false" onAction="#btnQuizzesPressed" text="Quizzes">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="btnViewResult" mnemonicParsing="false" onAction="#btnViewResultPressed" text="View Result">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </left>
   <center>
      <StackPane fx:id="stpCenter" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
